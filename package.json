{
  "name": "svem",
  "icon": "icons/svem-icon.png",
  "displayName": "Svem for VS Code",
  "description": "Language support for Svem (Svelte in Markdown).",
  "version": "0.0.2",
  "publisher": "Beerush",
  "author": "Nanang Mahdaen El Agung <mahdaen@gmail.com>",
  "engines": {
    "vscode": "^1.97.0"
  },
  "main": "scripts/index.js",
  "categories": [
    "Programming Languages"
  ],
  "devDependencies": {
    "vscode": "^1.1.37"
  },
  "activationEvents": [
    "onLanguage:svem"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "svem",
        "path": "./snippets/svem.code-snippets"
      }
    ],
    "languages": [
      {
        "id": "svem",
        "aliases": [
          "Svem",
          "svem",
          "Svelte Markdown",
          "svelte markdown",
          "svex"
        ],
        "extensions": [
          ".svem"
        ],
        "icon": {
          "light": "./icons/svem-icon.svg",
          "dark": "./icons/svem-icon.svg"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "svem",
        "scopeName": "text.svem",
        "path": "./syntaxes/svem.tmLanguage.json",
        "embeddedLanguages": {}
      },
      {
        "scopeName": "text.svem.codeblock",
        "path": "./syntaxes/svem-code-block.tmLanguage.json",
        "injectTo": [
          "text.svem"
        ],
        "embeddedLanguages": {}
      }
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/beerush-id/svem-vscode.git"
  },
  "keywords": [
    "svem",
    "svx",
    "svelte",
    "markdown",
    "svelte",
    "in",
    "markdown"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beerush-id/svem-vscode/issues"
  },
  "homepage": "https://github.com/beerush-id/svem-vscode#readme"
}
